    "prebuild": "rm -rf build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync"
  },
  "devDependencies": {
    "@contentful/rich-text-html-renderer": "^15.0.0",
    "@contentful/rich-text-types": "^15.0.0",
