{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  },
  // Excludes merged with .svelte-kit/tsconfig.json
  "exclude": [
    "./scripts/*",
    "./node_modules/**",
    "./.svelte-kit/[!ambient.d.ts]**"
  ]
}
